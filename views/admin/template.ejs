    <script>
        //Privacy Score Stackked Bar Chart

        window.onload = function () {
            var chart = new CanvasJS.Chart("privacy-score-chart", {
                animationEnabled: true,
                theme: "light1", //"light1", "dark1", "dark2"

                axisY: {
                    interval: 10,
                    suffix: "%"
                },
                toolTip: {
                    shared: false
                },
                data: [{
                    type: "stackedBar100",
                    toolTipContent: "<b>{name}:</b> {y} (#percent%)",
                    showInLegend: true,
                    name: "Score",
                    dataPoints: [
                        { y: 300, label: "Score" },
                    ]
                },
                {
                    type: "stackedBar100",
                    toolTipContent: "<b>{name}:</b> {y} (#percent%)",
                    showInLegend: true,
                    name: "Total",
                    dataPoints: [
                        { y: 756, label: "Total" },
                    ]
                },
                ]
            });
            chart.render();

        }

    </script>


