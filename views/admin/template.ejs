    <script>
        //initialize popovers for notes

        $(function () {
            $('[data-toggle="popover"]').popover({
                html: true
            })

                .on("mouseenter", function () {
                    var _this = this;
                    $(this).popover("show");
                    $(".popover").on("mouseleave", function () {
                        $(_this).popover('hide');
                    });
                })

                .on("mouseleave", function () {
                    var _this = this;
                    setTimeout(function () {
                        if (!$(".popover:hover").length) {
                            $(_this).popover("hide");
                        }
                    }, 500);
                });
        })

        $('.popover-dismiss').popover({
            trigger: 'click'
        })

    </script>

    <script>
        $(function () {
            $("#switch-mfa").change(function () {
                if ($(this).is(":checked")) {
                    $(".contentB").show();
                    $(".contentA").hide();
                } else {
                    $(".contentB").hide();
                    $(".contentA").show();
                }
            });
        });
    </script>

